{
    "name": "agielks/yii2-wablas",
    "description": "Wablas wrapper for Yii2",
    "type": "yii2-extension",
    "keywords": ["yii2-wablas", "yii2", "wablas", "component"],
    "authors": [
        {
            "name": "Agiel K. Saputra",
            "email": "agielkurniawans@gmail.com"
        }
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=7.4.0",
        "yiisoft/yii2": "~2.0.0",
        "yiisoft/yii2-httpclient": "~2.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5.0"
    },
    "autoload": {
        "psr-4": {
            "agielks\\yii2\\wablas\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "agielks\\yii2\\wablas\\tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
